{
  "name": "wegot-3.0",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webdriver-update": "node ./node_modules/webdriver-manager update --ignore_ssl",
    "webdriver-start": "node ./node_modules/webdriver-manager start",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "protractor-run": "protractor ./build/config/protractor.config.js",
    "suites": "protractor ./build/config/protractor.config.js --suites smoke",
    "logoreplace": "cpx resources/custom-logo.svg ./node_modules/allure-commandline/dist/plugins/custom-logo-plugin/static",
    "stylereplace": "cpx resources/styles.css ./node_modules/allure-commandline/dist/plugins/custom-logo-plugin/static",
    "ymlreplace": "cpx resources/allure.yml ./node_modules/allure-commandline/dist/config",
    "replacereportfiles": "npm-run-all logoreplace stylereplace ymlreplace --continue-on-error",
    "history": "mkdir reports\\htmlreports\\history || npm run replacereportfiles || ncp  reports/htmlreports/history reports/allure-results/history",
    "posttest": "npm run history && allure generate reports/allure-results --clean -o reports/htmlreports && allure open reports/htmlreports || true"
  },
  "author": "Naveenprakash Jayachandran",
  "license": "ISC",
